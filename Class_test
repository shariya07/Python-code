class student:
  def __init__(self,name,rollno):
    self.name= name
    self.rollno=rollno

  def display(self):
    print('name of student-',self.name)
    print('rollno of student-',self.rollno)

  def setage(self,age):
    self.age = age
    print('age is-',self.age)
  def setMarks(self,marks):
    self.marks=marks
    print('Marks -',self.marks)

obj1=student('shubham',4)
obj1.display()
obj1.setMarks(30)
obj1.setage(50)
